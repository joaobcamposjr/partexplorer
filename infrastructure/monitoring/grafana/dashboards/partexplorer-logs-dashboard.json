{
  "dashboard": {
    "id": null,
    "title": "PartExplorer - Logs",
    "tags": ["partexplorer", "logs", "loki"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Backend Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"docker\", container_name=~\".*backend.*\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Frontend Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"docker\", container_name=~\".*frontend.*\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "All Error Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"docker\"} |= \"error\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "GET Requests",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"docker\"} |= \"GET\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "POST Requests",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"docker\"} |= \"POST\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Performance Related Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"docker\"} |~ \"(duration|time|performance|slow)\"",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
