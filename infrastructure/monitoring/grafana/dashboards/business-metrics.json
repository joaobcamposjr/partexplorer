{
  "dashboard": {
    "id": null,
    "title": "Business Metrics - PartExplorer",
    "tags": ["business", "partexplorer", "analytics"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Visitors Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(analytics_pageviews_total[24h]))",
            "legendFormat": "Daily Visitors"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "gradient-gauge",
              "orientation": "auto",
              "reduceOptions": {"values": false, "calcs": ["lastNotNull"], "fields": ""}
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Session Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(rate(analytics_session_duration_seconds[1h]))",
            "legendFormat": "Avg Session Duration"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "gradient-gauge",
              "orientation": "auto"
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Bounce Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(analytics_bounce_total[1h]) / rate(analytics_pageviews_total[1h]) * 100",
            "legendFormat": "Bounce Rate %"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "gradient-gauge",
              "orientation": "auto"
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "count(analytics_active_users_total)",
            "legendFormat": "Active Users"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "gradient-gauge",
              "orientation": "auto"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Page Views Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(analytics_pageviews_total[5m])",
            "legendFormat": "Page Views/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "yAxes": [
          {"label": "Page Views/sec", "min": 0},
          {"show": false, "min": 0}
        ]
      },
      {
        "id": 6,
        "title": "Top Pages",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (page) (rate(analytics_pageviews_total[24h])))",
            "legendFormat": "{{page}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "page": "Page",
                "Value": "Views"
              }
            }
          }
        ]
      },
      {
        "id": 7,
        "title": "Search Analytics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(analytics_search_total[5m])",
            "legendFormat": "Searches/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "yAxes": [
          {"label": "Searches/sec", "min": 0},
          {"show": false, "min": 0}
        ]
      },
      {
        "id": 8,
        "title": "Part Views",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(analytics_part_views_total[5m])",
            "legendFormat": "Part Views/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "yAxes": [
          {"label": "Part Views/sec", "min": 0},
          {"show": false, "min": 0}
        ]
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(analytics_errors_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "yAxes": [
          {"label": "Errors/sec", "min": 0},
          {"show": false, "min": 0}
        ]
      },
      {
        "id": 10,
        "title": "Geographic Distribution",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (country) (rate(analytics_pageviews_total[24h]))",
            "legendFormat": "{{country}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "country": "Country",
                "Value": "Views"
              }
            }
          }
        ]
      },
      {
        "id": 11,
        "title": "Device Types",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (device_type) (rate(analytics_pageviews_total[24h]))",
            "legendFormat": "{{device_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 12,
        "title": "Performance Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(analytics_page_load_time_bucket[5m]))",
            "legendFormat": "95th percentile load time"
          },
          {
            "expr": "histogram_quantile(0.50, rate(analytics_page_load_time_bucket[5m]))",
            "legendFormat": "50th percentile load time"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "yAxes": [
          {"label": "Load Time (ms)", "min": 0},
          {"show": false, "min": 0}
        ]
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "time_range",
          "type": "interval",
          "query": "1m,5m,10m,30m,1h,6h,12h,1d,7d,30d",
          "current": {"text": "1h", "value": "1h"}
        }
      ]
    }
  }
}
